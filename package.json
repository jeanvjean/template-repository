{
  "name": "open-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "nodemon --exec babel-node --max-old-space-size=8192 src/app.js",
    "test": "BOILER_NODE_ENV=test mocha --timeout 100000 --require @babel/register tests/test --exit",
    "lint": "eslint --fix src/ --ext .js ",
    "pm2": "pm2 start pm2.json",
    "build": "babel src -d dist",
    "start": "babel-node --max-old-space-size=8192 src/app.js",
    "create:migration": "node node_modules/db-migrate/bin/db-migrate --config database.json create",
    "migrate_down": "db-migrate reset -c 100 --config database.json",
    "migrate_up": "db-migrate up --config database.json",
    "create:seed": "db-migrate create:seed -c 100 --config database.json",
    "seed:down": "db-migrate reset:seed -c 100 --config database.json",
    "seed:up": "db-migrate up:seed --config database.json",
    "migrate:fresh": "npm run migrate_down && npm run migrate_up",
    "pretest": "cross-env BOILER_NODE_ENV=test npm run migrate_down && cross-env BOILER_NODE_ENV=test npm run migrate_up && cross-env BOILER_NODE_ENV=test npm run seed:down && cross-env BOILER_NODE_ENV=test npm run seed:up"
  },
  "author": "enyata",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1189.0",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "csv-2-json": "^0.1.6",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "detect-port": "^1.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs-extra": "^10.1.0",
    "http-status": "^1.5.2",
    "joi": "^17.4.0",
    "json2csv": "^5.0.7",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.7.5",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "nodemailer-smtp-transport": "^2.7.4",
    "pg-promise": "^10.10.1",
    "request": "^2.88.2",
    "request-ip": "^2.1.3",
    "winston": "^3.3.3",
    "winston-papertrail": "git+https://github.com/kenperkins/winston-papertrail.git#v2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "~7.7.5",
    "@babel/node": "~7.7.4",
    "@babel/preset-env": "~7.7.6",
    "@babel/register": "~7.7.4",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.3.2",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.4"
  }
}
